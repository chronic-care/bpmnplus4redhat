    <distributionManagement>
		<repository>
			<id>hc-nexus-releases</id>
			<name>releases</name>
			<url>https://repository.openplatform.healthcare/repository/maven-releases/</url>
		</repository>
		<snapshotRepository>
			<id>hc-nexus-snapshots</id>
			<name>snapshots</name>
			<url>https://repository.openplatform.healthcare/repository/maven-snapshots/</url>
		</snapshotRepository>
